    "ngast": "^0.2.4",
    "rxjs": "~6.3.3",
    "typescript": "~3.1.6"
  },
  "devDependencies": {
    "@types/node": "^10.12.18",
    "copy": "^0.3.2",
    "rimraf": "^2.6.3"
