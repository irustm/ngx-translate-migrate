{
  "name": "ngx-translate-migrate",
  "version": "0.0.0",
  "description": "Automate migrate from ngx-translate to i18n",
  "repository": {
    "type": "git",
    "url": "https://github.com/irustm/ngx-translate-migrate.git"
  },
  "keywords": [
    "angular",
    "ngx-translate",
    "i18n",
    "translate",
    "migrate"
  ],
  "author": "irustm",
